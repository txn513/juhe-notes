page {
  background: #f6f6f6;
}
.container {
  padding-bottom: 120rpx;
  
}
.container.iphoneX {
  padding-bottom: 186rpx;
}
.container .wrap {
  /* margin-top: 10rpx; */
  background: #fff;
  width: 100%;
}
.single-note-wrap {
  box-sizing: border-box;
  padding: 20rpx 20rpx 10rpx;
  width: 690rpx;
  margin: 0 auto;
  /* border-radius: 4px; */
  background: #fff;
  /* box-shadow: 5rpx 10rpx 20rpx #eee,-5rpx 0 20rpx #eee; */
  font-weight: 400;
  text-align: left;
  border-bottom: 1px solid #f6f6f6;
}

.single-note-top {
  /* max-height: 400rpx; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 60rpx;
  line-height: 60rpx;
}
.single-note-top text {
  /* line-height: 50rpx; */
  font-weight: 500;
}
.single-note-sub {
  height: 40rpx;
  line-height: 40rpx;
  text-overflow: ellipsis;
   white-space: nowrap;
   overflow: hidden;
   color: #888;
}
.single-note-sub text{
  line-height: 40rpx;
  font-size: 22rpx;
  
  
}
.single-note-date {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 12px;
  position: relative;
  color: #888;
}
