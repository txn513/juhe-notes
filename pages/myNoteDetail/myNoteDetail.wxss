.single-note-wrap {
  box-sizing: border-box;
  padding: 20rpx 20rpx 10rpx;
  width: 700rpx;
  margin: 14px auto 0;
  border-radius: 4px;
  background: #fff;
  box-shadow: 5rpx 10rpx 20rpx #eee,-5rpx 0 20rpx #eee;
  font-weight: 400;
  text-align: left;
}
.single-note-date {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 12px;
  position: relative;
  color: #888;
}
.btn-wrap {
  margin-top: 50rpx;
}
.btn-wrap button {
  height: 100rpx;
  width: 700rpx;
  line-height: 100rpx;
  font-size: 12px;
  position: relative;
  color: #888;
}